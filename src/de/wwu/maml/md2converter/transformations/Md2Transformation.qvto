modeltype MAML "strict" uses "http://de/wwu/md2dot0";
modeltype MD2 "strict" uses "http://www.wwu.de/md2/framework/MD2";

transformation Md2Transformation(in Source: MAML, out Target: MD2);

main() {
	Source.rootObjects()[UseCase] -> map UseCaseToModel();
}

mapping UseCase::UseCaseToModel(): MD2Model {
	package := self.map toPackage();
}

mapping UseCase::toPackage(): PackageDefinition {
	pkgName := self.title.replaceAll(" ", "") + "." + "model"
}
